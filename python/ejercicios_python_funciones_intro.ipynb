    "def suma(a,b):\n",
    "        \n",
    "    if a == (\"\") and b == (\"\"):\n",
    "        resultado = 0\n",
    "    elif a == (\"\"):\n",
    "        resultado = b\n",
    "    elif b == (\"\"):\n",
    "        resultado = a\n",
    "    else: \n",
    "        resultado = int(a) + int(b)\n",
    "        resultado = str(resultado)\n",
    "        \n",
    "    return resultado"
   ]

    "def nuevo_string(a):\n",
    "    a = a.lower()\n",
    "    lista_string = list(a)\n",
    "    keys = []\n",
    "    values = []\n",
    "\n",
    "    for char in lista_string:\n",
    "        if lista_string.count(char) <= 1:\n",
    "            keys.append(char)\n",
    "            values.append(\"(\")\n",
    "        else: \n",
    "            keys.append(char)\n",
    "            values.append(\")\")\n",
    "        \n",
    "        resultado = \"\".join(values)\n",
    "    \n",
    "    return resultado"
